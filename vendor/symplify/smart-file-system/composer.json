{
    "name": "symplify\/smart-file-system",
    "description": "Sanitized FileInfo with safe getRealPath() and other handy methods",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "nette\/utils": "^3.0",
        "symfony\/finder": "^4.4|^5.1",
        "symfony\/filesystem": "^4.4|^5.1"
    },
    "require-dev": {
        "nette\/finder": "^2.5",
        "phpunit\/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RectorPrefix20210120\\Symplify\\SmartFileSystem\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RectorPrefix20210120\\Symplify\\SmartFileSystem\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}